{
	"env": {
			"browser": true,
			"es2021": true
	},
	"rules": {
		"react/react-in-jsx-scope": "off",
		"camelcase": "error",
		"spaced-comment": "error",
		"quotes": ["error", "single"],
		"no-duplicate-imports": "error",
		"importOrder": ["^@core/(.*)$", "^@server/(.*)$", "^@ui/(.*)$", "^[./]"],
		"importOrderSeparation": true,
		"importOrderSortSpecifiers": true
	}
}